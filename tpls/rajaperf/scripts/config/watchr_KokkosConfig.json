{
	"plots" : {
		"files" : {
			"fileName": "RAJAPerf-timing*",
 			"type" : "xml",
			"ignoreOldFiles" : true,
 			"recurseDirectories" : false
		},
		"categories": [
			"kokkos_lambda"

		],		
		"plot" : [
			{
				"autoname" : {
					"useProperty" : "y/path"
				},
				"category" : "kokkos_lambda",
				"template" : "kokkos_template",
				"dataLines" : [
					{
						"name" : "Data Line",
						"x" : {
							"getPath": "*",
							"getElement" : "performance-report",
							"getKey" : "date",
							"unit" : "timestamp"
						},
						"y" : {
							"getElement" : "performance-report|timing",
							"getPath": "*/kokkos_perf_suite/*",
  							"getPathAttribute": "name",
							"getKey" : "kokkos_lambda",
							"unit" : "seconds",
							"strategy" : {
								"getFirstMatchOnly" : "false",
								"recurseChildGraphs" : "true"
							}
						},
						"color" : "202,77,77"
					}
				]
			}
		]
	},
    "graphDisplay": {
		"dbLocation" : "root",
		"page" : 1,
		"displayRange" : 30,
		"graphWidth" : 450,
		"graphHeight" : 450,
		"graphsPerRow" : 3,
		"graphsPerPage" : 15,
		"displayedDecimalPlaces" : 3
	}
}
